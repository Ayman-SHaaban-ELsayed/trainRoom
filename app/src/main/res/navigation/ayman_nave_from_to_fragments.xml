<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/ayman_nave_from_to_fragments"
    app:startDestination="@id/listOfOfUsersFragment">

    <fragment
        android:id="@+id/listOfOfUsersFragment"
        android:name="com.ayman.roomdatabasetraining.fragment.listusers.ListOfOfUsersFragment"
