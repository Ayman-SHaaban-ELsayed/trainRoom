        tools:layout="@layout/fragment_list_of_of_users" >
        <action
            android:id="@+id/action_listOfOfUsersFragment_to_addUserFragment"
            app:destination="@id/addUserFragment" />
    </fragment>
    <fragment
        android:id="@+id/addUserFragment"
        android:name="com.ayman.roomdatabasetraining.fragment.adduser.AddUserFragment"
        android:label="fragment_add_user"
        tools:layout="@layout/fragment_add_user" >
        <action
            android:id="@+id/action_addUserFragment_to_listOfOfUsersFragment"
            app:destination="@id/listOfOfUsersFragment" />
        <action
            android:id="@+id/action_addUserFragment_to_listOfOfUsersFragment2"
            app:destination="@id/listOfOfUsersFragment" />
    </fragment>
</navigation>